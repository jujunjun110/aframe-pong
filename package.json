{
    "name": "project-name",
    "version": "1.0.0",
    "description": "project-name",
    "main": "index.js",
    "scripts": {
        "start": "npm run dev",
        "build": "browserify src/require.js -t babelify -o dev/build.js && browserify src/app.js -t browserify-shader -t babelify -o dev/app.js",
        "dev": "budo src/app.js --live --open --dir dev -- -t browserify-shader -t babelify -o dev/app.js",
        "dist": "npm run build && rm -rf ./dist && cp -r dev dist && rm -rf dist/*.js && browserify -t uglifyify src/require_dist.js -o dist/build.js && sed -i '' -e 's#<script src=\"./app.js\"></script>##g' dist/index.html && npm run showdist",
        "showdist": "budo --open --dir dist",
        "lint": "eslint src; exit 0",
        "unboil": "node src/unboil.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jujunjun110/aframe-application-boilerplate.git"
    },
    "keywords": [
        "aframe",
        "aframe-component",
        "aframe-vr",
        "vr",
        "webvr"
    ],
    "author": "jujunjun110 <https://twitter.com/jujunjun110>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/jujunjun110/aframe-application-boilerplate/issues"
    },
    "homepage": "https://github.com/jujunjun110/aframe-application-boilerplate#readme",
    "devDependencies": {
        "babel-eslint": "^7.1.1",
        "babel-preset-es2015": "^6.22.0",
        "babelify": "^7.3.0",
        "browserify": "^14.0.0",
        "browserify-shader": "^0.1.2",
        "budo": "^9.4.7",
        "envify": "^4.0.0",
        "eslint": "^3.14.1",
        "inquirer": "^3.0.1",
        "shelljs": "^0.7.0",
        "shx": "^0.2.2",
        "snazzy": "^6.0.0",
        "uglifyify": "^3.0.4"
    },
    "dependencies": {
        "aframe": "^0.4.0"
    }
}

